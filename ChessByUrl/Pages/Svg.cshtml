@page "/svg/{ruleset?}/{state?}/{moves?}"
@model SvgModel

@{
    Layout = null;
    Response.ContentType = "image/svg+xml";
}

<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg">
    <rect x="10" y="10" width="80" height="80" style="fill:blue;stroke:pink;stroke-width:5;opacity:0.5" />
    <text x="50" y="50" font-family="Verdana" font-size="20" fill="white" text-anchor="middle" alignment-baseline="middle">
        @Model.Ruleset
    </text>
</svg>
